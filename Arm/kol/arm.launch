<launch>q
	<node pkg="rosserial_python" type="serial_node.py" name="arm_joint1" output="screen">
	<param name="port" value="/dev/ttyUSB4" />
	<param name="baud" value="76800" />
	</node>

	<node pkg="rosserial_python" type="serial_node.py" name="arm_joint2" output="screen">
	<param name="port" value="/dev/ttyUSB0" />
	<param name="baud" value="76800" />
	</node>

	<node pkg="rosserial_python" type="serial_node.py" name="arm_joint0" output="screen">
	<param name="port" value="/dev/ttyUSB3" />
	<param name="baud" value="76800" />
	</node>
</launch>
