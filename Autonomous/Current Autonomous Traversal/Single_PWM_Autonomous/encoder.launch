<launch>
  <node
    name = "right_middle_encoder"
    pkg = "rosserial_python"
    type = "serial_node.py"
    output = "screen">
          <param name = "port" value = "/dev/ttyUSB0"/>
          <param name = "baud" value = "9600"/>
  </node>
  <node
    name = "left_middle_encoder"
    pkg = "rosserial_python"
    type = "serial_node.py"
    output = "screen">
          <param name = "port" value = "/dev/ttyUSB1"/>
          <param name = "baud" value = "9600"/>
  </node>
  <node
    name = "right_back_encoder"
    pkg = "rosserial_python"
    type = "serial_node.py"
    output = "screen">
          <param name = "port" value = "/dev/ttyUSB2"/>
          <param name = "baud" value = "9600"/>
  </node>
  <node
    name = "left_back_encoder"
    pkg = "rosserial_python"
    type = "serial_node.py"
    output = "screen">
          <param name = "port" value = "/dev/ttyUSB3"/>
          <param name = "baud" value = "9600"/>
  </node>
  <node
    name = "mega"
    pkg = "rosserial_python"
    type = "serial_node.py"
    output = "screen">
          <param name = "port" value = "/dev/ttyUSB4"/>
          <param name = "baud" value = "76800"/>
  </node>
  <node
    name = "bno"
    pkg = "rosserial_python"
    type = "serial_node.py"
    output = "screen">
          <param name = "port" value = "/dev/ttyUSB5"/>
          <param name = "baud" value = "14400"/>
  </node>
</launch>
